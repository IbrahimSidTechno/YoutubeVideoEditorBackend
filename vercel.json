{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/videosend/(.*)", "dest": "/public/uploads/$1" }, 
      { "src": "/download/(.*)", "dest": "/public/uploads/$1" },  
      { "src": "/(.*)", "dest": "/" }  
    ]
  }
  