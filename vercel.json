{
  "version": 2,
  "builds": [
    {
      "src": "./index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/public/uploads/$1"
    },
    {
      "src": "/trim/(.*)",
      "dest": "/public/trim/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
